<header class="page-header clearfix">

  <!--Logo Wrapper-->
  <a href="<%= root_path %>" class="logo-wrapper">
    <%= image_tag "one_life_logo.png", class: "logo" %>
  </a>

  <!--Mobile Menu Icon-->
  <a href="#" class="mobile-nav-button">
    <i class="icon"></i>
  </a>

  <!--Menu Wrapper Starts-->
  <nav class="manu-wrapper">
    <ul>
      <li id="home">
        <%= link_to t('home'), root_path %>
      </li>
      <li id="wedding">
        <%= link_to t('about.wedding'), wedding_path %>
      </li>
      <li id="gallery">
        <%= link_to t('gallery'), gallery_path %>
      </li>
      <li id="about">
        <%= link_to t('about.about'), about_path %>
      </li>
      <li id="contact">
        <%= link_to t('contact'), contact_path %>
      </li>
    </ul>
  </nav>
  <!--Menu Wrapper End-->
</header>
